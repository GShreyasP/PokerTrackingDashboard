{
  "version": 2,
  "cleanUrls": true,
  "rewrites": [
    {
      "source": "/apple-touch-icon-v2.png",
      "destination": "/apple-touch-icon-v2.png"
    },
    {
      "source": "/assets/icon-192-v2.png",
      "destination": "/assets/icon-192-v2.png"
    },
    {
      "source": "/assets/icon-512-v2.png",
      "destination": "/assets/icon-512-v2.png"
    }
  ],
  "headers": [
    {
      "source": "/manifest.json",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/manifest+json"
        }
      ]
    },
    {
      "source": "/sw.js",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript"
        },
        {
          "key": "Service-Worker-Allowed",
          "value": "/"
        }
      ]
    },
    {
      "source": "/assets/icon-*.png",
      "headers": [
        {
          "key": "Content-Type",
          "value": "image/png"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/apple-touch-icon*.png",
      "headers": [
        {
          "key": "Content-Type",
          "value": "image/png"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}

